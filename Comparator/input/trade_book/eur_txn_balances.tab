1,country,char(2),not null
2,system_unit_id,int,not null
3,transaction_id,varchar(18),not null
4,cmtm,float,null
5,market_value,float,null
6,default_swap_current_spread,float,null
7,cmtm_value_date,datetime,null
8,adjusted_cmtm_flag,char(1),null
9,adjusted_cmtm,float,null
10,unadjusted_cmtm,float,null
11,default_swap_exec_spread,float,null

##END!!

SELECT 
bal.country , 
bal.system_unit_id , 
bal.transaction_id , 
bal.cmtm , 
bal.market_value , 
bal.default_swap_current_spread , 
bal.cmtm_value_date , 
bal.adjusted_cmtm_flag , 
bal.adjusted_cmtm , 
bal.unadjusted_cmtm , 
bal.default_swap_exec_spread
FROM eur_txn_balances bal, eur_transactions tr WHERE tr.transaction_id=bal.transaction_id AND tr.system_unit_id=bal.system_unit_id
AND deleted IS NULL 
AND tr.handoff_source_id IN (${handoff_source_id})

##END!!