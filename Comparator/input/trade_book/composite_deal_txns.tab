SELECT 
C.cd_transaction_id , 
C.host_system_id , 
C.transaction_id , 
C.handoff_source_id , 
C.a , 
C.b , 
C.c , 
C.minimum , 
C.tenr_under_const , 
C.tenr_contr_const , 
C.tenr_under_date , 
C.tenr_contr_date , 
C.crf , 
C.cmtm , 
C.mliv , 
C.psr , 
C.amount , 
C.status , 
C.psle_mliv , 
C.psle
FROM  eur_transactions A,
composite_deals B,
composite_deal_txns C
WHERE A.TRANSACTION_ID  = B.CD_TRANSACTION_ID
AND B.CD_TRANSACTION_ID = C.CD_TRANSACTION_ID
AND B.host_system_id    = C.cd_host_system_id
AND A.HANDOFF_SRC_ID IN (${handoff_source_id})

##END!!