	       <entry key="${TRADING_BOOK_INPUT}/TRNX_DERIV_RATE_RESETS.sql"           value="${TRADING_BOOK_OUTPUT}/rate_resets_oracle.csv"/>
	       <entry key="${TRADING_BOOK_INPUT}/TRNX_DERIV_COLLATERAL.sql"            value="${TRADING_BOOK_OUTPUT}/collateral_oracle.csv"/>
